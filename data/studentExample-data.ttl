@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix voc:  <http://www.example.org/voc/> .
@prefix ex:    <http://example.org/> .
# University instances
ex:Stanford rdf:type ex:University .
ex:Stanford ex:name "Stanford University" .

ex:MIT rdf:type ex:University .
ex:MIT ex:name "Massachusetts Institute of Technology" .

# Student instances
ex:JohnDoe rdf:type ex:Student .
ex:JohnDoe ex:name "John Doe" .
ex:JohnDoe ex:takesCourse ex:Math101 .
ex:JohnDoe ex:takesCourse ex:CompSci202 .
ex:JohnDoe ex:advisedBy ex:JaneSmith .
ex:JohnDoe ex:studiesAt ex:Stanford .

ex:JaneDoe rdf:type ex:Student .
ex:JaneDoe ex:name "Jane Doe" .
ex:JaneDoe ex:takesCourse ex:CompSci202 .
ex:JaneDoe ex:takesCourse ex:Physics101 .
ex:JaneDoe ex:advisedBy ex:JohnSmith .
ex:JaneDoe ex:studiesAt ex:MIT .

# Course instances
ex:Math101 rdf:type ex:Course .
ex:Math101 ex:name "Math 101" .

ex:CompSci202 rdf:type ex:Course .
ex:CompSci202 ex:name "Computer Science 202" .

ex:Physics101 rdf:type ex:Course .
ex:Physics101 ex:name "Physics 101" .

# Department instances
ex:MathDept rdf:type ex:Department .
ex:MathDept ex:name "Department of Mathematics" .
ex:MathDept ex:worksFor ex:Stanford .

ex:CompSciDept rdf:type ex:Department .
ex:CompSciDept ex:name "Department of Computer Science" .
ex:CompSciDept ex:worksFor ex:MIT .

ex:PhysicsDept rdf:type ex:Department .
ex:PhysicsDept ex:name "Department of Physics" .
ex:PhysicsDept ex:worksFor ex:MIT .

# Lecturer instances
ex:JackSmith rdf:type ex:Lecturer .
ex:JackSmith ex:name "Jack Smith" .
ex:JackSmith ex:worksFor ex:CompSciDept .

ex:JillSmith rdf:type ex:Lecturer .
ex:JillSmith ex:name "Jill Smith" .
ex:JillSmith ex:worksFor ex:MathDept .

# Professor instances
ex:JohnSmith rdf:type ex:Professor .
ex:JohnSmith rdf:type ex:Lecturer .
ex:JohnSmith ex:name "John Smith" .
ex:JohnSmith ex:docDegreeFrom ex:MIT .
ex:JohnSmith ex:worksFor ex:CompSciDept .
ex:JohnSmith ex:teacherOf ex:CompSci202 .

ex:JaneSmith rdf:type ex:Professor .
ex:JaneSmith ex:name "Jane Smith" .
ex:JaneSmith ex:docDegreeFrom ex:Stanford .
ex:JaneSmith ex:worksFor ex:MathDept .
ex:JaneSmith ex:teacherOf ex:Math101 .
