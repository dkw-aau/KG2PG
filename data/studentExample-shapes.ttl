@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix shape: <http://shaclshapes.org/> .
@prefix ex:    <http://example.org/> .
@prefix ub:    <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#> .


shape:CourseShape
    rdf:type       sh:NodeShape ;
    sh:property    shape:instanceTypeCourseShapeProperty ;
    sh:property    shape:nameCourseShapeProperty ;
    sh:targetClass ex:Course .

shape:DepartmentShape
    rdf:type       sh:NodeShape ;
    sh:property    shape:instanceTypeDepartmentShapeProperty ;
    sh:property    shape:nameDepartmentShapeProperty ;
    sh:property    shape:worksForDepartmentShapeProperty ;
    sh:targetClass ex:Department .

shape:LecturerShape
    rdf:type       sh:NodeShape ;
    sh:property    shape:docDegreeFromLecturerShapeProperty ;
    sh:property    shape:instanceTypeLecturerShapeProperty ;
    sh:property    shape:nameLecturerShapeProperty ;
    sh:property    shape:teacherOfLecturerShapeProperty ;
    sh:property    shape:worksForLecturerShapeProperty ;
    sh:targetClass ex:Lecturer .

shape:ProfessorShape
    rdf:type       sh:NodeShape ;
    sh:property    shape:docDegreeFromProfessorShapeProperty ;
    sh:property    shape:instanceTypeProfessorShapeProperty ;
    sh:property    shape:nameProfessorShapeProperty ;
    sh:property    shape:teacherOfProfessorShapeProperty ;
    sh:property    shape:worksForProfessorShapeProperty ;
    sh:targetClass ex:Professor .

shape:StudentShape
    rdf:type       sh:NodeShape ;
    sh:property    shape:advisedByStudentShapeProperty ;
    sh:property    shape:instanceTypeStudentShapeProperty ;
    sh:property    shape:nameStudentShapeProperty ;
    sh:property    shape:studiesAtStudentShapeProperty ;
    sh:property    shape:takesCourseStudentShapeProperty ;
    sh:targetClass ex:Student .

shape:UniversityShape
    rdf:type       sh:NodeShape ;
    sh:property    shape:instanceTypeUniversityShapeProperty ;
    sh:property    shape:nameUniversityShapeProperty ;
    sh:targetClass ex:University .

shape:advisedByStudentShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:minCount 1 ;
    sh:or       ( [ sh:NodeKind sh:IRI ;
                    sh:class    ex:Professor ; ]
                  [ sh:NodeKind sh:IRI ;
                    sh:class    ex:Lecturer ; ] ) ;
    sh:path     ex:advisedBy .

shape:docDegreeFromLecturerShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:University ;
    sh:node     shape:UniversityShape ;
    sh:path     ex:docDegreeFrom .

shape:docDegreeFromProfessorShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:University ;
    sh:minCount 1 ;
    sh:node     shape:UniversityShape ;
    sh:path     ex:docDegreeFrom .

shape:instanceTypeCourseShapeProperty
    rdf:type sh:PropertyShape ;
    sh:in    ( ex:Course ) ;
    sh:path  rdf:type .

shape:instanceTypeDepartmentShapeProperty
    rdf:type sh:PropertyShape ;
    sh:in    ( ex:Department ) ;
    sh:path  rdf:type .

shape:instanceTypeLecturerShapeProperty
    rdf:type sh:PropertyShape ;
    sh:in    ( ex:Lecturer ) ;
    sh:path  rdf:type .

shape:instanceTypeProfessorShapeProperty
    rdf:type sh:PropertyShape ;
    sh:in    ( ex:Professor ) ;
    sh:path  rdf:type .

shape:instanceTypeStudentShapeProperty
    rdf:type sh:PropertyShape ;
    sh:in    ( ex:Student ) ;
    sh:path  rdf:type .

shape:instanceTypeUniversityShapeProperty
    rdf:type sh:PropertyShape ;
    sh:in    ( ex:University ) ;
    sh:path  rdf:type .

shape:nameCourseShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path     ex:name .

shape:nameDepartmentShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path     ex:name .

shape:nameLecturerShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path     ex:name .

shape:nameProfessorShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path     ex:name .

shape:nameStudentShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path     ex:name .

shape:nameUniversityShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:path     ex:name .

shape:studiesAtStudentShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:University ;
    sh:minCount 1 ;
    sh:node     shape:UniversityShape ;
    sh:path     ex:studiesAt .

shape:takesCourseStudentShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:Course ;
    sh:minCount 1 ;
    sh:node     shape:CourseShape ;
    sh:path     ex:takesCourse .

shape:teacherOfLecturerShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:Course ;
    sh:node     shape:CourseShape ;
    sh:path     ex:teacherOf .

shape:teacherOfProfessorShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:Course ;
    sh:minCount 1 ;
    sh:node     shape:CourseShape ;
    sh:path     ex:teacherOf .

shape:worksForDepartmentShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:University ;
    sh:minCount 1 ;
    sh:node     shape:UniversityShape ;
    sh:path     ex:worksFor .

shape:worksForLecturerShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:Department ;
    sh:minCount 1 ;
    sh:node     shape:DepartmentShape ;
    sh:path     ex:worksFor .

shape:worksForProfessorShapeProperty
    rdf:type    sh:PropertyShape ;
    sh:NodeKind sh:IRI ;
    sh:class    ex:Department ;
    sh:minCount 1 ;
    sh:node     shape:DepartmentShape ;
    sh:path     ex:worksFor .


